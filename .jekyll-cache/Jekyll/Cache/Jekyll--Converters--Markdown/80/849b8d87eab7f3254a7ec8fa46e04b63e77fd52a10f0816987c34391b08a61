<span class="n">_entity_poly</span><span class="p">.</span><span class="n">entity_id</span>                                                                     <span class="p">[</span><span class="mi">1</span><span class="p">]</span>
<span class="n">_entity_poly</span><span class="p">.</span><span class="n">pdbx_strand_id</span>                                                                <span class="p">[</span><span class="n">A</span><span class="p">]</span>
<span class="n">_entity_poly</span><span class="p">.</span><span class="n">type</span>                                                             <span class="p">[</span><span class="n">polypeptide</span><span class="p">(</span><span class="n">L</span><span class="p">)]</span>
